        end)
        wait()
        end
        end
        end)
        wait(3)
        for _,v in pairs(tabeeas) do
            v:Destroy()
            end
        detect:Disconnect()
        detect2:Disconnect()
        val:Destroy()
        _G.cdr = false
        end
